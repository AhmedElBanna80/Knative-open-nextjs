{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "üöÄ Cold Start Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "pointSize": 5
          },
          "unit": "s",
          "displayName": "Startup Duration"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(kn_next_startup_duration_seconds_bucket{app=~\"$app\"}[$__rate_interval]))",
          "legendFormat": "p95 - {{app}} ({{cache_status}})",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(kn_next_startup_duration_seconds_bucket{app=~\"$app\"}[$__rate_interval]))",
          "legendFormat": "p50 - {{app}} ({{cache_status}})",
          "refId": "B"
        }
      ]
    },
    {
      "title": "üî• Cache Warm vs ‚ùÑÔ∏è Cold Starts",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "warm" },
            "properties": [
              {
                "id": "color",
                "value": { "fixedColor": "#73BF69", "mode": "fixed" }
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "cold" },
            "properties": [
              {
                "id": "color",
                "value": { "fixedColor": "#F2495C", "mode": "fixed" }
              }
            ]
          }
        ]
      },
      "targets": [
        {
          "expr": "count(kn_next_bytecode_cache_warm_start{app=~\"$app\"} == 1)",
          "legendFormat": "warm",
          "refId": "A"
        },
        {
          "expr": "count(kn_next_bytecode_cache_warm_start{app=~\"$app\"} == 0)",
          "legendFormat": "cold",
          "refId": "B"
        }
      ]
    },
    {
      "title": "üìä Startup Duration Comparison (Warm vs Cold)",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(kn_next_startup_duration_seconds_bucket{app=~\"$app\", cache_status=\"warm\"}[$__rate_interval]))",
          "legendFormat": "üî• Warm p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(kn_next_startup_duration_seconds_bucket{app=~\"$app\", cache_status=\"cold\"}[$__rate_interval]))",
          "legendFormat": "‚ùÑÔ∏è Cold p95",
          "refId": "B"
        }
      ]
    },
    {
      "title": "üìÅ Cached Bytecode Files",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "green", "value": 50 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "kn_next_bytecode_cache_files_total{app=~\"$app\"}",
          "legendFormat": "{{app}} ({{build_id}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "üíæ Cache Size on Disk",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 1048576 },
              { "color": "yellow", "value": 52428800 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "kn_next_bytecode_cache_size_bytes{app=~\"$app\"}",
          "legendFormat": "{{app}} ({{build_id}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "‚úèÔ∏è Cache Writes (rate)",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          }
        }
      },
      "targets": [
        {
          "expr": "rate(kn_next_bytecode_cache_write_count{app=~\"$app\"}[$__rate_interval])",
          "legendFormat": "{{app}} writes/s",
          "refId": "A"
        }
      ]
    },
    {
      "title": "‚è±Ô∏è Process Uptime",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "orange", "value": null }, { "color": "green", "value": 60 }]
          }
        }
      },
      "targets": [
        {
          "expr": "process_uptime_seconds{app=~\"$app\"}",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "üß† Memory Usage (RSS)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          }
        }
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{app=~\"$app\"}",
          "legendFormat": "RSS - {{pod}}",
          "refId": "A"
        },
        {
          "expr": "nodejs_heap_size_used_bytes{app=~\"$app\"}",
          "legendFormat": "Heap Used - {{pod}}",
          "refId": "B"
        }
      ]
    },
    {
      "title": "‚ö° Event Loop Lag",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.1 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds{app=~\"$app\"}",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["kn-next", "bytecode", "knative", "cold-start"],
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "Prometheus",
          "value": "Prometheus"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "queryValue": "",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(kn_next_startup_duration_seconds_count, app)",
        "hide": 0,
        "includeAll": true,
        "label": "Application",
        "multi": true,
        "name": "app",
        "query": "label_values(kn_next_startup_duration_seconds_count, app)",
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "kn-next Bytecode Cache",
  "uid": "kn-next-bytecode-cache",
  "version": 1,
  "weekStart": ""
}
